{
  "author": "bgarciaoliveira",
  "name": "selektra",
  "version": "1.0.4",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build": "npm run build:cjs && npm run build:esm",
    "build:docs": "typedoc --out docs src/ --name 'Selektra' --entryPointStrategy expand",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest"
  },
  "keywords": [
    "selektra",
    "finder",
    "css-selector",
    "web-scraping",
    "xpath",
    "browser-automation",
    "xpath-selector",
    "dom-selector",
    "dom-utils",
    "css-selector-generator",
    "xpath-selector-generator",
    "dom-selector-generator",
    "element-selection",
    "element-selection-generator",
    "element-selection-builder",
    "selector-optimization",
    "selector-builder"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgarciaoliveira/selektra.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "bugs": {
    "url": "https://github.com/bgarciaoliveira/selektra/issues"
  },
  "homepage": "https://github.com/bgarciaoliveira/selektra#readme",
  "dependencies": {
    "linkedom": "^0.18.5"
  }
}
